{
  "name": "@stinkstudios/react-scripts",
  "version": "1.0.18-beta.0",
  "description": "Configuration and scripts for Create React App.",
  "repository": "stinkstudios/create-react-app",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "bugs": {
    "url": "https://github.com/stinkstudios/create-react-app/issues"
  },
  "files": [
    "bin",
    "config",
    "scripts",
    "template",
    "utils"
  ],
  "bin": {
    "react-scripts": "./bin/react-scripts.js"
  },
  "dependencies": {
    "@stinkstudios/eslint-config-react-app": "^0.3.0",
    "@stinkstudios/stylelint-config-react-app": "^0.1.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "caniuse-lite": "^1.0.30000766",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.14.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-compat": "2.1.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.0.4",
    "object-assign": "4.1.1",
    "postcss": "6.0.14",
    "postcss-cssnext": "3.0.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.9",
    "prettier": "1.9.2",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "raven-js": "3.21.0",
    "react-dev-utils": "^4.2.1",
    "style-loader": "0.19.1",
    "stylelint": "8.4.0",
    "stylelint-formatter-pretty": "1.0.3",
    "stylelint-webpack-plugin": "0.9.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.7",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-sentry-plugin": "1.14.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "optionalDependencies": {
    "fsevents": "1.1.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public"
  ],
  "stylelint": {
    "ignoreFiles": [
      "./node_modules/**",
      "./public/**"
    ],
    "extends": "@stinkstudios/stylelint-config-react-app",
    "rules": {
      "plugin/no-unsupported-browser-features": [
        true,
        {
          "ignore": [
            "css-animation",
            "transforms2d"
          ]
        }
      ]
    }
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "postcss": {
    "ident": "postcss",
    "plugins": {
      "postcss-flexbugs-fixes": {},
      "postcss-cssnext": {
        "features": {
          "autoprefixer": {
            "flexbox": "no-2009"
          }
        }
      }
    }
  }
}
